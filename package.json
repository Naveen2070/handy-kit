{
  "name": "@handykit/cli",
  "version": "0.4.4",
  "description": "A collection of handy CLI tools",
  "main": "dist/cli/index.js",
  "bin": {
    "handy-kit": "dist/cli/index.js"
  },
  "engines": {
    "node": ">=22.18.0"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Naveen2070/handy-kit.git"
  },
  "prepare": "npm run build",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cli",
    "license",
    "git",
    "standup"
  ],
  "author": "Naveen R",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc && npm run copy-assets",
    "test": "npm run build && vitest",
    "coverage": "npm run build && vitest run --coverage",
    "copy-assets": "cpx \"assets/**/*\" dist/assets"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^22.18.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "cpx": "^1.5.0",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.1",
    "rimraf": "^6.0.1",
    "tempy": "^3.1.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4"
  }
}
